{
  "name": "my-addon",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --ext js --exec \"babel-node ./src/app.js\"",
    "dev-debug": "nodemon --watch src --ext js --exec \"sleepms 5000 && babel-node --inspect=0.0.0.0:9229 ./src/app.js\"",
    "build": "babel src -d dist/src --no-comments --copy-files --ignore node_modules && cp package.json dist",
    "test": "mocha --require @babel/register --require @babel/polyfill --slow 30 --timeout 3000 --check-leaks --recursive \"src/**/*.test.js\"",
    "prod": "node dist/src/app.js",
    "cover": "nyc --check-coverage npm run test",
    "db": "knex",
    "migrate": "sleep 30 && yarn db migrate:latest",
    "create-migration": "CREATE_MIGRATION=true yarn db migrate:make",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">=12.x"
  },
  "dependencies": {
    "apollo-server-express": "^2.5.0",
    "atlassian-connect-express": "^3.5.1",
    "atlassian-jwt": "^1.0.2",
    "body-parser": "^1.19.0",
    "casual": "^1.6.0",
    "cls-hooked": "^4.2.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-redis-subscriptions": "^2.1.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.4",
    "helmet": "^3.18.0",
    "inversify": "^5.0.1",
    "inversify-vanillajs-helpers": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.11",
    "longjohn": "^0.2.12",
    "pg": "^7.11.0",
    "pg-pool": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.2",
    "request-promise-native": "^1.0.7",
    "sequelize": "^5.15.1",
    "sleep-ms": "^2.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.2",
    "flow-bin": "^0.100.0",
    "graphql-cli": "^3.0.11",
    "knex": "^0.19.5",
    "mocha": "^6.1.4",
    "ngrok": "^3.2.4",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "sequelize-mock": "^0.10.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2"
  }
}
